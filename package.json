{
  "name": "rubin-nest-project",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prestart:prod": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "dev": "NODE_ENV=dev nodemon",
    "start": "NODE_ENV=dev node -r ./tsconfig-paths-bootstrap.js dist/main.js",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"npm start\"",
    "start:debug": "NODE_ENV=dev tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "NODE_ENV=dev node -r ./tsconfig-paths-bootstrap.js dist/main.js",
    "lint": "tslint --project tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint-fix": "tslint --fix -c tslint.json --project tsconfig.json 'src/**/*{.ts,.tsx}'"
  },
  "dependencies": {
    "@nestjs/common": "^6.6.3",
    "@nestjs/core": "^6.6.7",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.7.1",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/typeorm": "^6.1.3",
    "bcrypt": "^3.0.6",
    "cache-manager": "^2.10.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.0",
    "compression": "^1.7.4",
    "helmet": "^3.20.1",
    "minio": "^7.0.12",
    "multer": "^1.4.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.2",
    "swagger-ui-express": "^4.1.1",
    "typeorm": "^0.2.18"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.10",
    "@nestjs/testing": "6.6.7",
    "@types/express": "4.17.1",
    "@types/jest": "24.0.18",
    "@types/node": "^12.12.6",
    "@types/supertest": "2.0.8",
    "jest": "24.9.0",
    "prettier": "1.18.2",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "tsc-watch": "3.0.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.19.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "3.5.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
